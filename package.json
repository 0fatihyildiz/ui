{
  "name": "siri-ui",
  "author": "Your name <yourmail@mail.com> (https://github.com/[USERNAME])",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Your package description",
  "private": false,
  "type": "module",
  "contributors": [
    {
      "name": "Asil",
      "email": "asil.kilic@siriusgrup.com"
    },
    {
      "name": "Fatih",
      "email": "fatih.yildiz@siriusgrup.com"
    }
  ],
  "keywords": [
    "ui",
    "sirius",
    "vue3",
    "vite",
    "storybook",
    "headless"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sirius-tedarik/ui.git"
  },
  "homepage": "https://sirius-tedarik.github.io/ui/",
  "bugs": {
    "url": "https://github.com/sirius-tedarik/ui/issues"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/siri-ui.umd.cjs",
  "module": "./dist/siri-ui.js",
  "exports": {
    ".": {
      "import": "./dist/siri-ui.js",
      "require": "./dist/siri-ui.umd.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "npm run test && vite build --mode production && npm run generate:types && npm run build:pages",
    "build:lib": "npm run test && vite build --mode production && npm run generate:types",
    "clean": "rm -rf ./dist && rm -rf ./live-demo && rm -rf ./coverage",
    "coverage:web": "http-server ./coverage",
    "generate:types": "vue-tsc -p tsconfig-build.json --declaration --emitDeclarationOnly true --outdir ./dist",
    "changeset": "changeset",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run  --coverage",
    "build:pages": "npm run docs:build && npm run build:live-demo",
    "build:live-demo": "vite build --mode live-demo --outDir ./docs/.vitepress/dist/live-demo",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "vue": "^3.3.8"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.25.0",
    "@changesets/cli": "^2.26.2",
    "@chromatic-com/storybook": "^1.6.1",
    "@storybook/addon-essentials": "^8.2.8",
    "@storybook/addon-interactions": "^8.2.8",
    "@storybook/addon-links": "^8.2.8",
    "@storybook/addon-onboarding": "^8.2.8",
    "@storybook/blocks": "^8.2.8",
    "@storybook/test": "^8.2.8",
    "@storybook/vue3": "^8.2.8",
    "@storybook/vue3-vite": "^8.2.8",
    "@types/node": "^20.9.4",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "9.5.0",
    "eslint-plugin-storybook": "^0.8.0",
    "storybook": "^8.2.8",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitepress": "^1.0.0-rc.24",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
